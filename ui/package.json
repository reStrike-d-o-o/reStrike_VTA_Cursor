{
  "name": "restrike-vta-ui",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "main": "src/App.tsx",
  "scripts": {
    "start": "craco start",
    "start:fast": "set GENERATE_SOURCEMAP=false && craco start",
    "build": "craco build",
    "build:fast": "set GENERATE_SOURCEMAP=false && craco build",
    "dev:fast": "craco start",
    "dev:network": "set HOST=0.0.0.0 && craco start",
    "test": "craco test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx}",
    "clean": "rm -rf build node_modules/.cache",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "i18n:sync": "node scripts/sync-i18n.mjs"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@dnd-kit/core": "^6.3.1",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/plugin-dialog": "^2.3.1",
    "framer-motion": "^11.18.2",
    "lottie-react": "^2.4.1",
    "obs-websocket-js": "^5.0.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.2",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "react-scripts": "^5.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5"
  },
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
