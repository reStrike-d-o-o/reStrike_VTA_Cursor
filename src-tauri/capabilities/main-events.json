{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-event-capability",
  "description": "Allow event listen/emit for main window",
  "windows": ["main"],
  "permissions": [
    "core:event:default",
    "shell:allow-open",
    "dialog:allow-message",
    "dialog:allow-confirm"
  ],
  "remote": {
    "urls": ["http://localhost:3000"]
  }
} 